<h1>Zlatos</h1>
  <body> 
  <p>Welcome! Here are some of our users' favorite movies!</p>
  <table class='table'>
  <thead>
    <tr>
      <th>Title</th>
      <th>Added By:</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
    <% @movies.each do |movie| %>
      <% @user = User.find_by(id: movie.user_id) %>
      <tr>
        <td><%= movie.title %></td>
        <td><%= link_to @user.username, @user %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
   
  </body>


